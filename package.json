{
  "name": "tujjarappsales",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "license": "MIT",
  "keywords": [],
  "author": "",
  "scripts": {
    "clean": "rimraf ./build",
    "build": "tsc -p .",
    "build:ormconfig": "copyfiles -f ormconfig.js  dist/",
    "watch": "tsc -w",
    "start": "node build/index.js | pino-pretty --colorize",
    "start:dev": "nodemon --config nodemon.json",
    "start:prod": "ts-node -r dotenv/config -r tsconfig-paths/register src/main.ts",
    "start-ts": "nodemon src/index.ts | pino-pretty --colorize",
    "dev": "nodemon --watch './src/**/*.ts' --exec ts-node ./src/main.ts",
    "test": "echo \"Error: no test specified\" && exit 1",
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts",
    "gen-proto": "$(npm bin)/proto-loader-gen-types --longs=String --enums=String  --oneofs --grpcLib=@grpc/grpc-js --outDir=src/proto/interfaces proto/*.proto",
    "prisma": "dotenv -e .env prisma",
    "db:gen": "npm run prisma generate",
    "db:studio": "npm run prisma studio",
    "db:migrate": "npx prisma migrate dev --name user-entity --create-only && npx prisma generate",
    "db:push": "npm run prisma db push",
    "db:deploy": "npm  run prisma migrate deploy",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "orm:windows:migrate": "rd /s /q build && yarn build && yarn typeorm migration:generate ./src/migrations/added-user-entity -d ./src/utils/data-source.ts",
    "orm:windows:push": "rd /s /q build && yarn build && yarn typeorm migration:run -d src/utils/data-source.ts",
    "orm:mac:migrate": "rm -rf build && yarn build && yarn typeorm migration:generate ./src/migrations/added-user-entity -d ./src/utils/data-source.ts",
    "orm:mac:push": "rm -rf build && yarn build && yarn typeorm migration:run -d src/utils/data-source.ts",
    "seed:generate": "ts-node ./node_modules/typeorm-seeding/dist/cli.js generate ",
    "seed:config": "ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run": "ts-node ./node_modules/typeorm-seeding/dist/cli.js seed -c  ./src/utils/data-source.ts"
  },
  "dependencies": {
    "@fastify/static": "^6.10.2",
    "@grpc/grpc-js": "^1.8.13",
    "@grpc/proto-loader": "^0.7.6",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^10.1.3",
    "@nestjs/config": "^3.0.0",
    "@nestjs/core": "^10.1.3",
    "@nestjs/jwt": "^10.1.0",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.1.3",
    "@nestjs/platform-fastify": "^10.1.3",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/swagger": "^7.1.6",
    "@nestjs/typeorm": "^10.0.0",
    "@types/config": "^3.3.0",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/multer": "^1.4.7",
    "bcrypt": "^5.1.0",
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "config": "^3.3.9",
    "crypto": "^1.0.1",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.3",
    "dotenv-cli": "^7.2.1",
    "express": "^4.18.2",
    "fluent-json-schema": "^4.1.0",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "mongoose": "^7.0.3",
    "mysql": "^2.18.1",
    "nodemon": "^2.0.22",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.10.0",
    "pino": "^8.11.0",
    "rate-limiter-flexible": "^3.0.0",
    "rxjs": "^7.8.0",
    "save-dev": "^0.0.1-security",
    "swagger-ui-express": "^4.6.2",
    "tsconfig-paths": "^4.2.0",
    "typeorm": "^0.3.14",
    "typeorm-seeding": "^1.6.1",
    "uid": "^2.0.2",
    "unique-username-generator": "^1.2.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cli-color": "^2.0.2",
    "@types/config": "^3.3.0",
    "@types/faker": "^6.6.9",
    "@types/node": "^18.15.11",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/passport-jwt": "^3.0.9",
    "@types/pino": "^7.0.5",
    "pino-pretty": "^10.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^4.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.3"
  }
}
